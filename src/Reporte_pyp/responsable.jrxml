				<text><![CDATA[NOMBRE:]]></text>
			</staticText>
			<staticText>
				<reportElement x="13" y="23" width="48" height="10"/>
				<textElement>
					<font fontName="Calibri" size="7" isBold="true"/>
				</textElement>
				<text><![CDATA[PARENTESCO:]]></text>
			</staticText>
			<staticText>
				<reportElement x="285" y="23" width="48" height="10"/>
